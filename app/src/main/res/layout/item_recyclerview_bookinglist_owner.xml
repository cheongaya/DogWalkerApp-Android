<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingTop="10dp"
    android:paddingLeft="20dp"
    android:paddingBottom="10dp"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:layout_marginBottom="5dp"
    android:background="@drawable/circular_corner_stroke_lightsky">

    <TextView
        android:id="@+id/textView_item_booking_walker_name_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="예약 도그워커"
        android:textColor="@color/colorBlack"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/textView_item_booking_walker_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="권은진"
        android:layout_marginLeft="10dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@id/textView_item_booking_walker_name_text"/>

    <TextView
        android:id="@+id/textView_item_booking_owner_dog_name_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="예약 강아지"
        android:textStyle="bold"
        android:layout_marginTop="3dp"
        android:textColor="@color/colorBlack"
        app:layout_constraintTop_toBottomOf="@id/textView_item_booking_walker_name"
        app:layout_constraintLeft_toLeftOf="parent"/>


    <ImageView
        android:id="@+id/imageView_item_booking_owner_dog_icon"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="10dp"
        android:src="@drawable/icon_dogwalker01"
        app:layout_constraintTop_toBottomOf="@id/textView_item_booking_walker_name"
        app:layout_constraintLeft_toRightOf="@id/textView_item_booking_owner_dog_name_text"/>

    <TextView
        android:id="@+id/textView_item_booking_owner_dog_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="후추"
        android:textColor="@color/colorSub"
        android:textStyle="bold"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toBottomOf="@id/textView_item_booking_walker_name"
        app:layout_constraintLeft_toRightOf="@id/imageView_item_booking_owner_dog_icon"/>

    <TextView
        android:id="@+id/textView_item_booking_walk_total_time_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="산책시간"
        android:textStyle="bold"
        android:layout_marginTop="3dp"
        android:textColor="@color/colorBlack"
        app:layout_constraintTop_toBottomOf="@id/textView_item_booking_owner_dog_name"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/textView_item_booking_walk_total_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="90분"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toBottomOf="@id/textView_item_booking_owner_dog_name"
        app:layout_constraintLeft_toRightOf="@id/textView_item_booking_walk_total_time_text"/>

    <TextView
        android:id="@+id/textView_item_booking_walk_time_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="예약일시"
        android:textStyle="bold"
        android:textColor="@color/colorBlack"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toBottomOf="@id/textView_item_booking_walk_total_time"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/textView_item_booking_walk_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5시 10분"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toBottomOf="@id/textView_item_booking_walk_total_time"
        app:layout_constraintLeft_toRightOf="@id/textView_item_booking_walk_time_text"/>

</androidx.constraintlayout.widget.ConstraintLayout>